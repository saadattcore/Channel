              <add policyType="Emaratech.Services.Identity.Wcf.RPP.JwtAuthorizationPolicy, Emaratech.Services.Identity.Wcf.RPP" />
            </authorizationPolicies>
          </serviceAuthorization>
        </behavior>
      </serviceBehaviors>
