      <service name="Emaratech.Services.Channels.ChannelService">
        <endpoint address="soap" binding="basicHttpBinding" bindingConfiguration="basicHttp" contract="Emaratech.Services.Channels.Contracts.Rest.IChannelService" />
        <endpoint address="json" binding="webHttpBinding" contract="Emaratech.Services.Channels.Contracts.Rest.IChannelService" bindingConfiguration="webHttp" />
        <endpoint address="soap/monitor" binding="basicHttpBinding" bindingConfiguration="basicHttp" contract="Emaratech.Services.WcfCommons.HealthCheck.IHealthCheck" />
        <endpoint address="json/monitor" binding="webHttpBinding" contract="Emaratech.Services.WcfCommons.HealthCheck.IHealthCheck" bindingConfiguration="webHttp" />
      </service>
      <service name="Emaratech.Services.Channels.ChannelServiceV2">
        <endpoint address="soap/v2" binding="basicHttpBinding" bindingConfiguration="basicHttp" contract="Emaratech.Services.Channels.Contracts.Rest.IChannelService" />
        <endpoint address="json/v2" binding="webHttpBinding" contract="Emaratech.Services.Channels.Contracts.Rest.IChannelService" bindingConfiguration="webHttp" />
        <endpoint address="soap/monitor/v2" binding="basicHttpBinding" bindingConfiguration="basicHttp" contract="Emaratech.Services.WcfCommons.HealthCheck.IHealthCheck" />
        <endpoint address="json/monitor/v2" binding="webHttpBinding" contract="Emaratech.Services.WcfCommons.HealthCheck.IHealthCheck" bindingConfiguration="webHttp" />
      </service>
    </services>
    <extensions>
